{
    "name": "farazasifali/larapress",
    "description": "A simple laravel blog with markdown",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fraz Ali",
            "email": "farazasifali@gmail.com"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "farazasifali\\Larapress\\": "src/",
            "farazasifali\\Larapress\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "farazasifali\\Larapress\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "farazasifali\\Larapress\\LarapressBaseServiceProvider"
            ]
        }
    },
    "require": {
        "erusev/parsedown": "^1.7"
    }
}
